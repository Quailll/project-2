<div class="p-3">
  <h2>Your Feed</h2>
</div>

<div class="container">
  <div class="row justify-content-center">
    <div class="col-lg-8">
      
  {{#each shoes}}
  <div class="card m-5 text-center" style="width: 25rem;">
    <h5>Posted by {{user.username}}</h5>
    <img src="{{image}}" class="card-img-top" alt="panda">
    <div class="card-body">
      <h5 class="card-title">{{name}}</h5>
    </div>
    <ul class="list-group list-group-flush">
      <li class="list-group-item">{{brand}}</li>
      <li class="list-group-item">{{style}}</li>
    </ul>
  </div>
  {{/each}}

    </div>
  </div>
</div>

